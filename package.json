{
  "name": "nosflare",
  "version": "7.5.16",
  "main": "worker.js",
  "type": "module",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outfile=worker.js --platform=neutral --target=es2020 --format=esm --minify=false --keep-names --external:node:*",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit",
    "test": "tsc --project tsconfig.test.json && node --test dist/tests/*.js"
  },
  "dependencies": {
    "@noble/curves": "^1.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "wrangler": "^3.0.0"
  }
}
